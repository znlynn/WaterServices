@model IEnumerable<WaterServices.Models.WaterData>

@{
    ViewData["Title"] = "Stream Data";
}

<h2>Index</h2>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Discharge)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GageHeight)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Temp)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Discharge)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GageHeight)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Temp)
            </td>
        </tr>
}
    </tbody>
</table>
